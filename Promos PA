WITH filters AS (SELECT
/* Specify below the dates and country that need to be checked, this filter
   is for Query execution optimization, nothing else needs to be changed. */
                     TIMESTAMP '2024-12-16 00:00:00 UTC' AS Start_date,
                     TIMESTAMP '2024-12-22 00:00:00 UTC' AS End_date,
                     'ES'              AS Country)
-------------------------------------------------------------------------------------------
SELECT fod.order_id                       AS Order_id,
       fom.order_country_code             AS Country_code,
       fom.store_address_id               AS Store_address_id,
       fom.order_activated_local_at       AS Activation_time_local,
       fom.order_is_marketplace           AS Marketplace,
       --pd.discount_subtype                AS Promotion_type,
       SUM(pd.product_discount_assumed_by_partner_currency_local) AS Products_assumed_by_partner,
       SUM(pd.delivery_discount_assumed_by_partner_currency_local) AS DF_assumed_by_partner_with_vat,
       SUM(pd.delivery_discount_assumed_by_partner_currency_local / (1.0 + fom.order_tax_rate_adj)) AS DF_assumed_by_partner_without_vat


FROM delta.finance_financial_metrics_odp.financial_order_discounts fod
         CROSS JOIN filters f
         JOIN delta.finance_financial_metrics_odp.financial_order_metrics fom
              ON fod.order_id = fom.order_id
         LEFT JOIN delta.growth_pricing_discounts_odp.pricing_discounts pd
                   ON fom.order_id = pd.order_id
         LEFT JOIN delta.partner_commissions_odp.order_base_commissions obc
                   ON fom.order_id = obc.order_id

WHERE 1 = 1
  AND fom.order_final_status IN ('DeliveredStatus', 'CanceledStatus')
  AND fom.order_activated_local_at IS NOT NULL
  AND fom.order_activated_local_at >= f.Start_date
  AND fom.order_activated_local_at < f.End_date
  AND fom.order_country_code = f.Country
  AND pd.payment_strategy IN ('ASSUMED_BY_PARTNER', 'PARTNER_AND_GLOVO')
 -- AND fom.store_address_id IN (156102,190847,153231,153241,188417,104555,104556,104558,104561,104562,2004,185035,206230,207365,145788,216771,191768,207356,211141,219616,219673,222660,225968,226604,132507,132771,159535,159888,161797,227708,128853,90754,166829,127752,127771,127773,152547,23816,48760,209310,210093,214070,215001,608,6332,67122,83137,2490,2491,2492,116972,27502,37418,92609,102042,45926,67009,125834,204980,127834,141027,176249,184688,186454,141038,176250,184708,186455,176256,184710,234263,234279,177417,185547,198705,222320,222321,222677,199310,235051,216741,207329,219350,231671,229424,231576,127429,127430,153009,159809,159847,159872,159907,161660,161676,161824,185845,204737,219420,219422,122061,188392,188402,1760,10968,20359,54975,137559,162908,6844,25688,35188,145647,197274,222642,186840,27594,127535,188405,29308,80285,94895,94897,111146,132527,47195,144068,222690,118856,167504,185841,185842,152551,138198,17842,235318,235328,235546,235563,236677,237071,237075,237232,239262,240016,240031,241559,243814,244077,244084,244121,245594,245717,246097,246100,246872,246911,247960,247962,248735,251304,251810,252574,253144,253152,253153,253154,253157,253262,253372,253388,253394,253399,253403,253406,253409,253411,253412,254859,255007,255054,255056,255152,255154,255161,255222,255253,255254,255261,255263,255264,255266,255267,255270,255271,255273,255275,255278,255282,255283,255285,255288,255605,255810,255864,255945,255960,255984,255988,255993,256002,256349,256366,256639,256996,257002,257003,257553,258183,258184,258187,258409,258963,259665,259667,259698,261266,261771,263004,265917,265918,269341,270496,275919,278010,278018,279770,281360,281361,281364,281365,281367,281370,281373,281375,281682,282082,282219,282230,282578,282659,282670,282710,282711,283463,284368,286620,286625,286631,286634,286915,286972,291581,291582,291583,292018,292027,294025,298902,298948,300290,300482,301119,302280,302735,302766,303006,303009,303079,303127,303165,303960,305781,309832,309865,310958,3135